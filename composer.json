{
  "name": "yudhatamaaditiyara/github-actions-codecov-php-test",
  "description": "yudhatamaaditiyara/github-actions-codecov-php-test",
  "require": {
    "php": ">=7.2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5|^9.3"
  },
  "autoload": {
    "psr-4": {
      "Test\\": "src/"
    }
  },
  "scripts": {
    "test": "phpunit -c test/phpunit.config.xml",
    "coverage": "phpunit --coverage-text --coverage-clover=build/coverage/coverage-clover.xml -c test/phpunit.config.xml"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}